@model Supplier
<div class="card">
    <div class="card-body">
        <h4 class="card-title">@Model.Name</h4>
        <p class="card-text">@Model.Id</p>
        <p class="card-text">@Model.Address</p>
        <p class="card-text">@Model.Email</p>
        <p class="card-text">@Model.PhoneNumber</p>
    </div>
</div>